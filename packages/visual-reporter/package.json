{
  "name": "@wdio/visual-reporter",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "bin": {
    "visual-reporter": "./dist/cli.js"
  },
  "scripts": {
    "build": "run-s clean build:scripts",
    "build:report": "npm run generate-thumbnails && npm next-build",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "clean": "rimraf coverage dist .next out",
    "dev": "npm run generate-thumbnails && run-p watch:scripts next-dev",
    "export": "npm run generate-thumbnails && NEXT_EXPORT=true next build",
    "generate-thumbnails": "node ./src/app/scripts/generateThumbnails.mjs",
    "test:lint": "next lint",
    "next-build": "next build",
    "next-dev": "next dev",
    "serve": "next start",
    "static": "npm run export && npx serve out",
    "start": "next start",
    "watch": "run-s clean watch:*",
    "watch:dev": "npm run dev",
    "watch:scripts": "tsc -p tsconfig.scripts.json --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "next": "14.2.5",
    "ora": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-select": "^5.8.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {}
}