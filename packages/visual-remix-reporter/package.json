{
  "name": "visual-remix-reporter",
  "author": "Wim Selles - wswebcreation",
  "description": "Visual Testing HTML Report for the @wdio/visual-service module",
  "version": "0.3.0",
  "license": "MIT",
  "homepage": "https://webdriver.io/docs/visual-testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/visual-testing.git"
  },
  "type": "module",
  "bin": {
    "wdio-visual-reporter": "./dist/cli.js"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "build:report": "remix vite:build",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "clean": "rimraf coverage build *.tsbuildinfo",
    "dev": "npm run generate-thumbnails && run-p watch:scripts dev:remix",
    "dev:remix": "remix vite:dev",
    "generate-thumbnails": "node ./app/scripts/generateThumbnails.mjs",
    "test:lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "watch": "run-s clean watch:*",
    "watch:dev": "npm run dev",
    "watch:scripts": "tsc -p tsconfig.scripts.json --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "isbot": "^4.1.0",
    "next": "14.2.14",
    "ora": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-select": "^5.8.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}